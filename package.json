{
    "name": "megafon-contest-symbols-couter",
    "version": "0.0.0",
    "private": true,
    "license": "MIT",
    "author": "Dmitry <dmitry@cojam.ru> (https://e965.ru)",
    "repository": "github:e965/megafon-contest-symbols-couter",
    "homepage": "https://e965.github.io/megafon-contest-symbols-couter",
    "scripts": {
        "build": "preact build --no-sw",
        "serve": "sirv build --cors --single",
        "start": "preact watch",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d build",
        "lint": "eslint src",
        "prettier": "prettier --write **/*.{html,json,js,jsx,css,scss}",
        "lint-staged": "lint-staged"
    },
    "eslintIgnore": [
        "build/*"
    ],
    "lint-staged": {
        "*.{html,json,js,jsx,css,scss}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint-staged"
        }
    },
    "dependencies": {
        "preact": "^10.1.0",
        "preact-render-to-string": "^5.1.2"
    },
    "devDependencies": {
        "eslint": "^7.0.0",
        "eslint-config-preact": "^1.1.1",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.3",
        "gh-pages": "^2.2.0",
        "husky": "^4.2.5",
        "lint-staged": "^10.2.2",
        "preact-cli": "^3.0.0-rc.6",
        "prettier": "^2.0.5",
        "sass": "^1.26.5",
        "sass-loader": "^8.0.2",
        "sirv-cli": "^0.4.5"
    }
}
